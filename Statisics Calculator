import numpy as np

def calculate(list):
    """Calculates the mean, variance, stdev, max, min and sum of a list"""
    
    mydict = {'Mean': np.mean(list), 'Variance': np.var(list), 'Standard Deviation': np.std(list), 'Max': np.max(list), 'Min': np.min(list), 'Sum': np.sum(list)}
    
    return mydict

calculate([0,1,2,3,4,5,6,7,8])
